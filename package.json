{
  "name": "bbc-charts",
  "description": "BBC Charts UI library",
  "version": "0.0.1",
  "scripts": {
    "build": "sass _assets/sass/main.scss:_assets/css/style.min.css _assets/sass/charts.scss:_assets/css/charts.min.css --style=compressed",
    "watch": "sass --watch _assets/sass/:_assets/css/ _app/sass/:_app/css/ --style=compressed",
    "partials": "node _app/local_node_modules/partials.js"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "sass": "^1.55.0",
    "serve": "^14.0.1"
  }
}
